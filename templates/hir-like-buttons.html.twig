{% if drupal_token('site:linkedin') is not empty %}
    <a href="{{ drupal_token('site:linkedin') }}" target="_blank">
        <i class="fa-brands fa-linkedin fa-2x mx-1" aria-hidden="true" style="color: #0074B1;"></i>
    </a>
{% endif %}
